{
  "name": "nol",
  "version": "0.2.0",
  "description": "Null object class wrapper.",
  "main": "nol.js",
  "support": "nol.support.js",
  "deploy": "nol.deploy.js",
  "browser": "nol.deploy.js",
  "scripts": {
    "build:null": "babel null.module.js --out-file null.js --source-maps",
    "build:nol": "babel nol.js --out-file nol.support.js --source-maps",
    "build": "npm run build:null && npm run build:nol && webpack --progress --display-modules",
    "test": "mocha nol-test.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/volkovasystems/nol.git"
  },
  "keywords": [
    "null",
    "object",
    "class",
    "wrapper",
    "nol"
  ],
  "author": "Richeve S. Bebedor <richeve.bebedor@gmail.com>",
  "contributors": [
    "John Lenon Maghanoy <johnlenonmaghanoy@gmail.com>",
    "Vinse Vinalon <vinsevinalon@gmail.com>"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/volkovasystems/nol/issues"
  },
  "homepage": "https://github.com/volkovasystems/nol#readme",
  "dependencies": {
    "babel-runtime": "^6.23.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-latest": "^6.24.1",
    "source-map-loader": "^0.2.1",
    "webpack": "^3.0.0"
  }
